{
    "name": "magentron/laravel-api-logger",
    "description": "Laravel API logger to log responses and requests to file.",
    "type": "library",
    "keywords": ["laravel-api-logger", "laravel", "api", "logger"],
    "homepage": "https://github.com/Magentron/laravel-api-logger",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Jeroen Derks",
            "email": "jeroen@derks.it",
            "homepage": "https://www.derks.it/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "ext-json": "*",
        "laravel/framework": "^5.4|^6.20.14|^7|^8"
    },
    "require-dev": {
        "php": "^5.6|^7|^8",
        "clean/phpdoc-md": "9999999-dev",
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "orchestra/testbench": "^3.4.x-dev",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "9999999-dev",
        "phpunit/phpunit": "^5.7",
        "sebastian/phpcpd": "^3.0",
        "squizlabs/php_codesniffer": "^3.x-dev",
        "theseer/phpdox": "^0.11.2"
    },
    "autoload": {
        "psr-4": {
            "Magentron\\ApiLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Magentron\\ApiLogger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Magentron\\ApiLogger\\Providers\\ServiceProvider"
            ]
        }
    }
}
